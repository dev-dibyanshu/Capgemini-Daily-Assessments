using System;
class Vehical
{
  protected double distance=0.0;
  protected double hour=0.0;
  protected double fuel=0.0;

  public Vehical(double distance,double hour,double fuel)
  {
    this.distance=distance;
    this.hour=hour;
    this.fuel=fuel;
  }
  public void Average()
  {
    double average=0.0;
    average=disteance/fuel;

    Console.WriteLine("vehicle average is {0:0.00}",average);
  }
  public virtual void Speed()
  {
    double speed =0.0;
    speed=distance/hour;
    Console.WriteLine("Speed of the vehicle is {0:0.0}/kmp",speed);
  }
}